{
  "name": "mini-lib",
  "version": "1.0.0",
  "private": "true",
  "description": "mini lib",
  "scripts": {
    "start": "npm run watch:dev-script & npm run dev",
    "dev": "nodemon --watch $npm_package_config_develop_scripts $npm_package_config_develop_scripts/dev.js react-router",
    "watch:dev-script": "npm run compile:dev-script -- --watch",
    "compile:dev-script": "tsc mini-lib/dev --outDir $npm_package_config_develop_scripts --module CommonJS --target ESNext --downlevelIteration --esModuleInterop",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@babel/core": "^7.13.15",
    "@babel/parser": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/traverse": "^7.13.15",
    "faye-websocket": "^0.11.3",
    "fs-extra": "^9.1.0",
    "memfs": "^3.2.2",
    "open": "file:lib\\open",
    "resolve": "^1.20.0"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.14",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.11",
    "@types/node": "^14.14.37",
    "@types/resolve": "^1.20.0",
    "code-fabric": "^1.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "lint-staged": {
    "**/*.{js,ts,vue}": "eslint --ext .js,.ts,.vue",
    "**/*.{js,ts,md,json}": [
      "prettier --write"
    ]
  },
  "config": {
    "develop_scripts": "scripts/develop"
  }
}
