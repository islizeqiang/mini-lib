{
  "name": "mini-lib",
  "version": "1.0.0",
  "private": "true",
  "description": "mini lib",
  "scripts": {
    "dev": "nodemon --watch $npm_package_config_develop_scripts $npm_package_config_develop_scripts/dev.js $npm_package_config_entry",
    "watch:dev-script": "npm run compile:dev-script -- --watch --preserveWatchOutput",
    "compile:dev-script": "tsc mini-lib/dev --outDir $npm_package_config_develop_scripts --module CommonJS --target ESNext --downlevelIteration --esModuleInterop",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@babel/core": "^7.14.8",
    "@babel/parser": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/traverse": "^7.14.8",
    "faye-websocket": "^0.11.4",
    "fs-extra": "^10.0.0",
    "memfs": "^3.2.2",
    "open-browser": "link:lib/open-browser",
    "resolve": "^1.20.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@types/babel__core": "^7.1.15",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.12",
    "@types/node": "^15.14.3",
    "@types/resolve": "^1.20.1",
    "chalk": "^4.1.2",
    "code-fabric": "^1.5.0",
    "eslint": "^7.32.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.1.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx,vue}": "eslint --ext .js,.ts,.jsx,.tsx,.vue",
    "**/*.{js,ts,md,json}": [
      "prettier --write"
    ]
  },
  "config": {
    "develop_scripts": "scripts/develop",
    "entry": "react-render"
  }
}
